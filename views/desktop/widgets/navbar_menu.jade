ul#navbar_menu.nav
  - each item in menus.common.navbar
    - var ns = head.apiPath.split('.').shift(), item_ns = item.to.split('.').shift();
    - if (ns === item_ns)
      li(data-api3-route=item_ns, class='active'): a(href=item.link)= t(item.title)
    - else
      li(data-api3-route=item_ns): a(href=item.link)= t(item.title)
