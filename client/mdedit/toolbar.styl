.mdedit__toolbar
  margin-bottom 10px
  border-bottom 1px solid #e9e9e9
  padding 5px 0px 5px 5px

.mdedit-toolbar__item
  width 25px
  height 25px
  line-height 25px
  text-align center
  display inline-block
  cursor pointer
  overflow hidden
  font-size 15px

.mdedit-toolbar__sep
  width 1px
  border none
  background #e9e9e9
  cursor auto
  margin 0px 5px 0px 5px
