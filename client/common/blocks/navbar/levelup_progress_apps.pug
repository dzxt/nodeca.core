nav.navbar-primary.navbar.navbar-inverse.bg-primary.fixed-top.navbar-toggleable
  .navbar-inner.container.layout__container.container-fluid
    .navbar-untoggleable
      ul.navbar-nav
        li.nav-item
          a.navbar__level-up.nav-link.icon.icon-level-up(href=self.level_up)

      != self.partial('@common.blocks.navbar.blocks.page_progress', self.progress)

      button.navbar-toggler.navbar-toggler-right.icon.icon-menu(
        data-toggle='collapse',
        data-target='.navbar-collapse',
        type='button'
      )

    .navbar-collapse.collapse.navbar-toggleable-xs
      ul.navbar-nav
        != self.partial('@common.blocks.navbar.blocks.apps')

      ul.navbar-nav.navbar__right
        li.nav-item
          a.nav-link.icon.icon-search(href=self.link_to('search.general'))

        //- NOTE: This block is located in `nodeca.users`
        != self.partial('@common.blocks.navbar_auth')
